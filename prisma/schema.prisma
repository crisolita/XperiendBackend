// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
  id       Int     @id @default(autoincrement())
  fullName String
  email    String? @unique
  password String?
  authToken String?
  referall String @unique
  referallFriend String?
  mngpayId String?
}
model keys {
  id Int @id @default(autoincrement())
  wallet String
  key String
}